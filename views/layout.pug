doctype html
html(lang="en")
     head
          title JBC

          block styles

     body(data-spy="scroll" data-target=".site-navbar-target" data-offset="300" )
          .site-wrap#home-section.row
               .site-mobile-menu.site-navbar-target
                    .site-mobile-menu-header
                         .site-mobile-menu-close.mt-3
                              span.icon-close2.js-menu-toggle

                    .site-mobile-menu-body

               header.site-navbar.site-navbar-target(role="banner")
                    .container.mb-3
                         .d-flex.align-items-center
                              .site-logo.mr-auto
                                   a(href="index")
                                        img.img-responsive.logo(src="images/output-onlinepngtools.png" alt="logo")


                              .site-quick-contact.d-none.d-lg-flex.ml-auto
                                   .d-flex.site-info.align-items-center.mr-5
                                        span.block-icon.mr-3
                                             span.icon-map-marker
                                        span Le Palm Building,Plot 4, Lubobo Close, 
                                        | Muyenga

                                   .d-flex.site-info.align-items-center
                                        span.block-icon.mr-3
                                             span.icon-clock-o
                                        span Monday - Saturday 6:45AM - 9:00PM
                                        | Sunday CLOSED

                    .container
                         .menu-wrap.d-flex.align-items-center
                              span.d-inline-block.d-lg-none
                                   a.text-black.site-menu-toggle.js-menu-toggle.py-5(href="#")
                                        span.icon-menu.h3.text-black

                              nav.site-navigation.text-left.mr-auto.d-none.d-lg-block(role="navigation")
                                   ul.site-menu.main-menu.js-clone-nav.mr-auto
                                        li
                                             a.nav-link(href="index") Home
                                        li
                                             a.nav-link(href="aboutus") About
                                        li
                                             a.nav-link(href="packages") Packages
                                        li 
                                             a.nav-link(href="gallery") Gallery
                                        li 
                                             a.nav-link(href="pricing") Pricing
                                        li 
                                             a.nav-link(href="contact") Contact

                              .top-social.ml-auto
                                   a
                                        span.icon-facebook.text-teal
                                   a
                                        span.icon-twitter.text-success
                                   a
                                        span.icon-linkedin.text-yellow

