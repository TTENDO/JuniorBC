                                        a.nav-link(href="#")
                                             i.fas.fa-cog Settings
                                        a.nav-link(href="login")
                                             i.fas.fa-power-off Logout

          .row
               .col-sm-2.withinSecondRow
                    nav
                         ul
                              li
                                   a(title="dashboard" href="dashboard")
                                        span.fas.fa-home
                                        span.mini-sub-pro Home

                              //-supervisor tab
                              li.dropdown
                                   a
                                        span.fas.fa-user-graduate
