     body.container-fluid(style='height:100%;width:100%;background:url("images/lotion.jpg");background-position:center;background-repeat:no-repeat;background-size:cover;opacity:2;height:100vh;color:white')

          .row.firstRow
               .col-sm-2(style="background-color:white")
                    img.img-responsive(src="images/hugelogo.png" style="width:200px;height:60px" alt="Image")

               .col-sm-10.withinFirstRow
                    .row
                         .col-sm-10 
                              .row
                                   .col-sm-4.theHeading
                                        h1 Junior
                                   .col-sm-4.theHeading
